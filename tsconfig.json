  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "lib": ["esnext"],
    "types": ["@cloudflare/workers-types"],
    "typeRoots": ["src/types"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "react-jsx",
    // "jsxFragmentFactory": "Fragment",
    "jsxImportSource": "hono/jsx"
  }
}
